<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE auton SYSTEM "auton.dtd">
<auton>

<primitive id = "RESET_POSITION_PATH_PLANNER"
pathname= "BlueAmpWing2Center0.path"/>

<primitive id= "DRIVE_PATH_PLANNER"
pathname= "BlueAmpWing2Center0.path"
time= "0.6"/>
<!--Drive closer to speaker and angle ourselves-->

<snippet file = "ShootWhenStop.xml"/>
<!--Shoot preload-->

<primitive id = "DRIVE_PATH_PLANNER"
pathname= "BlueAmpWing2Center0.path"
time= "3.0"/>
<!--Go up to ring, assumed to intake the 1.0 mark in this drive, then drive up to speaker and shoot-->

<snippet file = "ShootWhenStop.xml"/>

<primitive id = "DRIVE_PATH_PLANNER"
pathname= "BlueAmpWing2Center0.path"
time= "1.0"/>
<!--We go up to the ring in front of speaker and are assumed to intake it at this mark-->

<snippet file = "ShootWhenStop.xml"/>
<!--Shoot after intake and we are already across the line so we are done-->

</auton>