<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE auton SYSTEM "auton.dtd">
<auton>
        <primitive id="RESET_POSITION"
	            pathname="RedHPOne1.wpilib.json"
               intake="INTAKE_HOLD"/>
    <!--score preloaded cone-->
    <snippet file="ScoreCone.xml"/>

    <!--drive to charing station-->
    <primitive id="DRIVE_PATH"
               pathname="RedHPOne1.wpilib.json"
               headingOption="SPECIFIED_ANGLE"
               heading="0"
               arm="STARTING_POSITION_ROTATE"
               intake="INTAKE_OFF"
               time="0.5"
               />
    <primitive id="DRIVE_PATH"
               pathname="RedHPOne2.wpilib.json"
               headingOption="SPECIFIED_ANGLE"
               heading="0"
               intake="INTAKE_OFF"
               time="3.0"
               />
    <primitive id="DRIVE_PATH"
               pathname="RedHPOne4.wpilib.json"
               headingOption="SPECIFIED_ANGLE"
               heading="0"
               intake="INTAKE_OFF"
               time="5.0"
               />
    <primitive id="DRIVE_PATH"
               pathname="RedHPOne3.wpilib.json"
               headingOption="SPECIFIED_ANGLE"
               heading="179.5"
               intake="INTAKE_OFF"
               time="2.0"
               />
    <!--drive on charing station-->
    <primitive id="DRIVE_PATH"
               pathname="RedHPOne4Slow.wpilib.json"
               headingOption="SPECIFIED_ANGLE"
               heading="179.5"
               intake="INTAKE_OFF"
               time="10.0"
               />
    <!--auto balance-->
    <primitive id="AUTO_BALANCE"
               arm="STARTING_POSITION_ROTATE"
               extender="STARTING_POSITION_EXTEND"
               intake="INTAKE_OFF"
               time="10.0"
               />
</auton>