<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE auton SYSTEM "auton.dtd">
<auton>

<primitive id = "RESET_POSITION_PATH_PLANNER"
pathname = "BluePodiumWing1Center0.path"
/>

<primitive id= "DRIVE_PATH_PLANNER"
pathname = "BluePodiumWing1Center0.path"
time = "0.5"/>
<!--Drive forward .5 seconds + adjusts angle-->

<snippet file = "ShootWhenStop.xml"/>
<!--Stops and shoots-->

<primitive id= "DRIVE_PATH_PLANNER"
pathname= "BluePodiumWing1Center0.path"
time= "2.0"/>
<!--Drives and intakes the ring then goes to the speaker (Intake at 0.6 seconds after shooting the preload)-->

<snippet file= "StopWhenShoot.xml"/>

<primitive id = "DRIVE_PATH_PLANNER"
pathname= "BluePodiumWing1Center0"
time= "2.0"/>
<!--crosses line-->

</auton>