<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE auton SYSTEM "auton.dtd">
<auton>
   <primitive id="RESET_POSITION"
	            pathname="BlueWallOne1.wpilib.json"
               intake="INTAKE_HOLD"/>
    <!--score preloaded cone-->
    <snippet file="ScoreCone.xml"/>
    <!--drive to charing station-->
    <primitive id="DRIVE_PATH"
               pathname="RedWallOne1.wpilib.json"
               headingOption="SPECIFIED_ANGLE"
               heading="180"
               arm="STARTING_POSITION_ROTATE"
               extender="STARTING_POSITION_EXTEND"
               intake="INTAKE"
               time="0.8"
               />
    <primitive id="DRIVE_PATH"
               pathname="RedWallOne4.wpilib.json"
               headingOption="SPECIFIED_ANGLE"
               heading="0"
               arm="STARTING_POSITION_ROTATE"
               extender="STARTING_POSITION_EXTEND"
               intake="INTAKE"
               time="1.5"
               />
    <primitive id="DRIVE_PATH"
               pathname="RedWallTwo2Slow.wpilib.json"
               headingOption="SPECIFIED_ANGLE"
               heading="0"
               arm="STARTING_POSITION_ROTATE"
               extender="STARTING_POSITION_EXTEND"
               intake="INTAKE"
               time="5.0"
               />
    <primitive id="DRIVE_PATH"
               pathname="RedWallTwo1.wpilib.json"
               headingOption="SPECIFIED_ANGLE"
               heading="0"
               arm="STARTING_POSITION_ROTATE"
               extender="STARTING_POSITION_EXTEND"
               intake="INTAKE"
               time="3.0"
               />
    <!--pick up cube-->
    <primitive id="DO_NOTHING"
               headingOption="SPECIFIED_ANGLE"
               heading="0"
               arm="STARTING_POSITION_ROTATE"
               extender="STARTING_POSITION_EXTEND"
               intake="INTAKE"
               time="0.5"/>
   <!--drive to scoring station-->
    <primitive id="DRIVE_PATH"
               pathname="RedWallTwo2.wpilib.json"
               headingOption="SPECIFIED_ANGLE"
               heading="0"
               arm="STARTING_POSITION_ROTATE"
               extender="STARTING_POSITION_EXTEND"
               intake="INTAKE_HOLD"
               time="5.0"
               />
    <primitive id="AUTO_BALANCE"
               headingOption="SPECIFIED_ANGLE"
               heading="0"
               arm="STARTING_POSITION_ROTATE"
               extender="STARTING_POSITION_EXTEND"
               intake="INTAKE_HOLD"
               time="10.0"
               />
</auton>